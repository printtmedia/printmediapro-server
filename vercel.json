{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/send-order",
      "dest": "/api/send-order.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/root.js"
    }
  ]
}