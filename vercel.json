{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "methods": ["GET"],
      "dest": "/server.js"
    },
    {
      "src": "/api/send-order",
      "methods": ["OPTIONS"],
      "dest": "/server.js"
    },
    {
      "src": "/api/send-order",
      "methods": ["POST"],
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}